{
    "name": "circle.js",
    "title": "Circle.js",
    "version": "0.2.1",
    "homepage": "https://github.com/rawify/Circle.js",
    "bugs": "https://github.com/rawify/Circle.js/issues",
    "description": "A function collection for working with circles",
    "keywords": [
        "circle",
        "circles",
        "intersection",
        "area",
        "perimeter",
        "circle from three points"
    ],
    "private": false,
    "main": "./dist/circle.js",
    "module": "./dist/circle.mjs",
    "types": "./circle.d.ts",
    "browser": "./dist/circle.min.js",
    "unpkg": "./dist/circle.min.js",
    "readmeFilename": "README.md",
    "exports": {
        ".": {
            "types": "./circle.d.ts",
            "require": "./dist/circle.js",
            "import": "./dist/circle.mjs"
        }
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:rawify/Circle.js.git"
    },
    "funding": {
        "type": "github",
        "url": "https://github.com/sponsors/rawify"
    },
    "author": {
        "name": "Robert Eisele",
        "email": "robert@raw.org",
        "url": "https://raw.org/"
    },
    "license": "MIT",
    "engines": {
        "node": "*"
    },
    "directories": {
        "test": "tests"
    },
    "scripts": {
        "build": "crude-build Circle",
        "test": "mocha tests/*.js"
    },
    "devDependencies": {
        "crude-build": "^0.0.3",
        "mocha": "*",
        "should": "*"
    }
}
